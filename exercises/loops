contract LoopCheck {
    function thousandLoop(uint _n) external pure returns (uint){
        uint sum = 0;
        for(uint i = 1; i <= _n; i++){
            sum += i;
        }
        return sum;
    }

    function complexOperation(uint256 n) public pure returns (uint256) {
        uint256 result = 1;
        uint256 i = 1;
        
        while (i <= n) {
            result *= i;
            i++;
        }
        
        return result;
    }
    
    }
